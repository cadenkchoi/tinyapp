<header>
  <nav class="navbar navbar-expand-md navbar-light" style="background-color: #af2cf5;">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>
    <div>
      <% if (user_id) { %>
        <form method="POST" action="/logout">
          <label for="logout">
            <label style="margin-right:1rem">Currently logged in as: <b><%= user_id["email"] %></b></label>
          <button type="submit" class="btn btn-outline-danger">Logout</button></label>
        </form>
      <% } %>
      <% if (!user_id) { %>
        <div>
        <a href="/login" class="btn btn-info" role="button"> Login </a>
        <a href="/register" class="btn btn-info" role="button"> Register </a>
        </div>
      <% } %>
    </div>
  </nav>
</header>